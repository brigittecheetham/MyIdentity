@using System.Collections.Generic;

@model IEnumerable<Web.ViewModels.MemberViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Members</h2>

<table id="members" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Surname</th>
            <th>Cellphone</th>
            <th>Home Address</th>
            <th>Email Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>
                    @member.Name
                </td>
                <td>
                    @member.Surname
                </td>
                <td>
                    @member.Cellphone
                </td>
                <td>
                    @member.HomeAddress  
                </td>
                <td>
                    @member.UserName
                </td>
            </tr>
        }
    </tbody>
</table>
